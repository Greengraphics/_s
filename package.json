    "postcss": "^6.0.11",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "Safari >= 9"
  ],
  "stylelint" : {
    "extends": "stylelint-config-standard",
    "rules": {
      "indentation": 4,
      "comment-whitespace-inside": null,
      "selector-max-id": 0,
      "declaration-no-important": true
    }
  },
  "eslint" : {
    "extends": ["google"],
    "parserOptions": {
      "ecmaVersion": 5
    },
    "env": {
      "browser": true,
      "jquery": true
    },
    "rules": {
      "no-var": "off",
      "max-len": "warn",
      "require-jsdoc": "warn"
    }
  }
}